CC = gcc
CFLAGS = -g -O2 -DOS_LINUX -DDSO_DLFCN -DCPU=\"amd64\" -Wall -Wstrict-prototypes
CPPFLAGS = 
INCLUDES =  -I/usr/lib/jvm/java-8-openjdk-amd64/include -I/usr/lib/jvm/java-8-openjdk-amd64/include/linux
LDFLAGS = 
LIBS =  -ldl -lpthread
RANLIB = ranlib
LDCMD = gcc
STRIP = @: 

.c.o:
	$(CC) $(CFLAGS) $(CPPFLAGS) $(EXTRA_CFLAGS) $(INCLUDES) -c $< -o $@
